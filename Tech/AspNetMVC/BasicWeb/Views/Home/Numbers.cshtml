@{
    ViewData["Title"] = ViewBag.Title;
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

<div class="text-center">
    <h1 class="display-4">Числа</h1>
    @if (ViewBag.Nums != null)
    {
    <ul class="nums">
        @foreach (var num in ViewBag.Nums)
        {
            <li>@num</li>
        }
    </ul>
    }
    else
    {
        <div>Няма ги числата!</div>
    }
</div>
